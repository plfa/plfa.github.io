{
  "root": true,
  "elements": ["html5"],
  "extends": ["html-validate:recommended", "html-validate:document"],
  "rules": {
    "long-title": ["warn"],
    "no-dup-id": ["warn"],
    "no-inline-style": ["warn"],
    "prefer-native-element": ["warn"],
    "unique-landmark": ["off"],
    "hidden-focusable": ["off"],
    "valid-id": ["error", { "relaxed": true }],
    "heading-level": ["off"],
    "input-missing-label": ["off"],
    "wcag/h30": ["off"]
  }
}
